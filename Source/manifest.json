{
	"manifest_version": 2,

	"name": "DildoFarm",
    "short_name": "DF",
	"description": "Replaces 'YCombinator' with a name more befitting how seriously it should be taken.",
	"author": "John Berry",
	"version": "1.0",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
